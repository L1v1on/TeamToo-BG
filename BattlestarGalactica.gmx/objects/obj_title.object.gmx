            <string>//check for gamepads connected
 for (var i = 0; i &lt; gp_num; i++;)
    {
    if gamepad_is_connected(i) global.gp[i] = true else global.gp[i] = false;
    if (global.gp[i] = true)
        {
           if (gamepad_button_check_pressed(i, gp_padu))
           {
                if (selection &gt; 1)
                {
                    selection--
                    audio_play_sound(snd_select_move, 0, false);
                } 
           }
           if (gamepad_button_check_pressed(i, gp_padd))
           {
                if (selection &lt; 2)
                {
                    selection++
                    audio_play_sound(snd_select_move, 0, false);
                }
           }
           if (gamepad_button_check_pressed(i, gp_face1))
           {
                audio_play_sound(snd_select, 1, false);
           }
           if (gamepad_button_check_released(i, gp_face1))
           {
                direction = 270;
                gravity = 1.5;
           }
        }
    }

</string>
