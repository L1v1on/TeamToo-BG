    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>switch(Knightstate2){
case actionsKnight2.normal:

    if(global.p1_count==2){
        instance_destroy();
    }
    if(global.p2_count==2){
        instance_destroy();
    }
scr_controller_input(1,obj_aimer,6);

castAttack=gamepad_button_check_pressed(1,gp_shoulderrb);
castChain = gamepad_button_check_pressed(1,gp_shoulderr);
castSlam = gamepad_button_check_pressed(1,gp_face3);
castDash = gamepad_button_check_pressed(1,gp_face1);
castCrit = gamepad_button_check_pressed(1,gp_shoulderl);
//ShotgunChecker
if(place_meeting(x,y,obj_shotgun_hitbox)){
    global.hpknightP2-=100;
    motion_add(obj_player_trapper.direction,27);
}
//CritChecker
if(critActive==1){
    sprite_index=spr_player_crit;
}
else if(critActive==0){
    sprite_index=spr_knight;
}
//AfflictionChecker
if(p2afflicted==1){
    global.hpknightP2-=1;
    sprite_index = spr_player_afflicted;
}
//Dash
if(castDash){
        speed = 40;
}
if(castAttack){
    audio_play_sound(snd_sword_swing, 0, false);
    sword = instance_create(x,y,obj_sword);
    sword.direction = direction;
    sword.image_angle = direction;
    
    hitbox =instance_create(x,y,obj_sword_hitbox2);
    hitbox.direction=direction;
    hitbox.image_angle=direction;
    
    if(random(30)&gt;=29){
        critActive=1;
    }
}
if(castChain &amp;&amp; inProcess==0){
    chain = instance_create(x,y,obj_chain_creator2);
    chain.direction = direction;
    chain.speed=10;
    chain.image_angle=direction;
    inProcess=1;
    alarm[0]=100;//code for cooldown
}
if(castSlam &amp;&amp; inProcess2==0){
    instance_create(x,y,obj_slam_p2);
    inProcess2=1;
    
}
if(critActive==1 &amp;&amp; castCrit){
    audio_play_sound(snd_sword_swing, 0, false);
    sword = instance_create(x,y,obj_sword_crit);
    sword.direction = direction;
    sword.image_angle = direction;
    
    hitbox =instance_create(x,y,obj_sword_hitbox_crit2);
    hitbox.direction=obj_aimer.direction;
    hitbox.image_angle=direction;
    
    critActive=0;
}
break;
case actionsKnight2.grapple:

    move_towards_point(obj_grapple_location2.x,obj_grapple_location2.y,10);
    
break;
case actionsKnight2.frozen:
    sprite_index=spr_player_trapped;
    if(place_meeting(x,y,obj_shotgun_hitbox)){
    global.hpknightP2-=100;
    audio_play_sound(snd_player_hit, 0, false);
    motion_add(obj_player_trapper.direction,27);
}
if(p2afflicted==1){
    global.hpknightP2-=1;
    sprite_index = spr_player_afflicted;
}
    speed=0;
break;
case actionsKnight2.slam:
    move_towards_point(obj_slam_active_p2.x,obj_slam_active_p2.y,40);
break;
}//end switch
</string>
