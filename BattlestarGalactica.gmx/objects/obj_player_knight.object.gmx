            <string>switch(Knightstate){
case actionsKnight.normal:
scr_controller_input(0,obj_aimer,6);

castAttack=gamepad_button_check_pressed(0,gp_shoulderrb);
castChain = gamepad_button_check_pressed(0,gp_shoulderr);
castSlam = gamepad_button_check_pressed(0,gp_face3);
castDash = gamepad_button_check_pressed(0,gp_face1);
castCrit = gamepad_button_check_pressed(0,gp_shoulderl);
//CritChecker
if(critActive==1){
    sprite_index=spr_player_crit;
}
else if(critActive==0){
    sprite_index=spr_knight;
}
//AfflictionChecker
if(p1afflicted==1){
    global.hpknightP1-=1;
    sprite_index = spr_player_afflicted;
}
//Dash
if(castDash){
        speed = 40;
}
if(castAttack){
    sword = instance_create(x,y,obj_sword);
    sword.direction = direction;
    sword.image_angle = direction;
    
    hitbox =instance_create(x,y,obj_sword_hitbox);
    hitbox.direction=obj_aimer.direction;
    hitbox.image_angle=direction;
    
    if(random(30)&gt;=29){
        critActive=1;
    }
}
if(castChain &amp;&amp; inProcess==0){
    chain = instance_create(x,y,obj_chain_creator);
    chain.direction = direction;
    chain.speed=10;
    chain.image_angle=direction;
    inProcess=1;
    alarm[0]=100;//code for cooldown
}
if(castSlam &amp;&amp; inProcess2==0){
    instance_create(x,y,obj_slam_p1);
    inProcess2=1;
    
}
if(critActive==1 &amp;&amp; castCrit){
    sword = instance_create(x,y,obj_sword_crit);
    sword.direction = direction;
    sword.image_angle = direction;
    
    hitbox =instance_create(x,y,obj_sword_hitbox_crit);
    hitbox.direction=obj_aimer.direction;
    hitbox.image_angle=direction;
    
    critActive=0;
}
break;
case actionsKnight.grapple:
    move_towards_point(obj_grapple_location.x,obj_grapple_location.y,10);
break;
case actionsKnight.frozen:
    sprite_index=spr_player_trapped;
    speed=0;
break;
case actionsKnight.slam:
    move_towards_point(obj_slam_active_p1.x,obj_slam_active_p1.y,40);
break;
}//end switch
