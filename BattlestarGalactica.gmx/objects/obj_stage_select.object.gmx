            <string>for (var i = 0; i &lt; 2; i++;)
    {
    if (global.gp[i] = true)
        {
         if (picked = false)
          {
           if (gamepad_button_check_pressed(i, gp_padl))
           {
                if (stage_selected &gt; 1)
                {
                    stage_selected--
                    audio_play_sound(snd_select_move, 0, false);
                } 
           }
           if (gamepad_button_check_pressed(i, gp_padr))
           {
                if (stage_selected &lt; 3)
                {
                    stage_selected++
                    audio_play_sound(snd_select_move, 0, false);
                }
           }
           if (gamepad_button_check_pressed(i, gp_face1))
           {
                audio_play_sound(snd_select, 1, false);
                alarm[0] = 120;
                picked = true;
                stop = 1;
           }
          } 
        }
    }
